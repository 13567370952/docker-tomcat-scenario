<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>


    <servlet>
        <servlet-name>dockerEnvTestServlet</servlet-name>
        <servlet-class>com.a.eye.skywalking.docker.test.DockerEnvTestServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>routingTestServlet</servlet-name>
        <servlet-class>com.a.eye.skywalking.docker.test.RoutingTestServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>dockerEnvTestServlet</servlet-name>
        <url-pattern>/docker-env</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>routingTestServlet</servlet-name>
        <url-pattern>/routing/url-1</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>routingTestServlet</servlet-name>
        <url-pattern>/routing/url-2</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>routingTestServlet</servlet-name>
        <url-pattern>/routing/url-3</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>routingTestServlet</servlet-name>
        <url-pattern>/routing/url-4</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>/index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
